<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdtu.simpleexamine.mapper.ModuleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cdtu.simpleexamine.pojo.dbo.Module">
        <id column="module_id" property="moduleId"/>
        <result column="module_name" property="moduleName"/>
        <result column="module_code" property="moduleCode"/>
        <result column="module_parent" property="moduleParent"/>
        <result column="module_desc" property="moduleDesc"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        module_id, module_name, module_code, module_parent, module_desc, update_time
    </sql>

</mapper>
